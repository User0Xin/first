#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int main()
{
	int a, b, c, i;
	double d = 0;
	double e = 0;
	double f = 0;
	double sum;
	//printf("请输入a，b，c：");
	scanf("%d%d%d", &a, &b, &c);
	//用循环进行运算
	for (i = 1; i <= a; i++)
	{
		d+=i;
	}
	for (i = 1 ; i <= b; i++)
	{
		e += i*i;
	}
	for (i = 1 ; i <= c; i++)
	{
		f += 1.00/i;
	}
	sum = d + e + f;
	//输出结果
	printf("%.2lf", sum);
	return 0;
}
/*
	测试数据：
		100 50 10  47977.93
		1 1 2  3.50
*/
