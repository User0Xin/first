#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
#include<string.h>
#include<windows.h>
#include<stdlib.h>
int main()
{
	float j;
	float o;
	double oh = 0;
	int i = 1;
	for (i = 1; i <= 100; i++)
	{
		if (i % 2 == 0)
		{
			float o = 1.0 / i;
			oh = oh - o;
			//continue;
		}
		else if (i % 2 == 1)
		{
			float j = 1.0 / i;
			oh = oh + j;
		}
	}
	printf("%f\n", oh);
	return 0;
}
