#include<stdio.h>

int main()
{
	int x = 0;
	int y = 0;
	//printf("请输入x的值>: ");
	scanf("%d", &x);
	if (x < 1) //进行判断
	{
		printf("%d", x);
	}
	else if (1 <= x && x < 10)
	{
		y = 2 * x - 1;  //用y接收计算后的值
			if (y < 14)
			{
				y -= 5;
			}
			printf("%d", y);
	}
	else if (10 <= x)
	{
		y = 3 * x - 11;
		if (y < 25)
		{
			y *= 2;
		}
		printf("%d", y);
	}
	return 0;
}

/*测试数据：2 - 2
            3 0
			5 4
			*/
