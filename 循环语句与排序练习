#include<stdio.h>
#include<math.h>

int main()
{
	char a = 'a';
	char b = 'b';
	char c = 'c';
	while (scanf("%c%c%c", &a, &b, &c) != EOF)
	{
		getchar();
		char ret = 'a';
		int i = 0;
		if (a > b)
		{
			ret = a;
			a = b;
			b = ret;
		}
		if (b>c)
		{
			ret = b;
			b = c;
			c = ret;
		}
		if (a > b)
		{
			ret = a;
			a = b;
			b = ret;
		}
		printf("%c %c %c\n", a, b, c);
	}
}
